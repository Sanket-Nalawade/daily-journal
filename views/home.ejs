<%- include('partials/header'); -%>
<div class="main">
<h1>Home</h1>
<p><%= content %></p>
<% posts.forEach(function(post){ %>
  <h1><%= post.title %></h1>
  <p><%= post.post.substring(0, 100) + " ..." %><a href="/posts/<%= post._id %>">Read More</a></p>
<% }); %>
</div>
